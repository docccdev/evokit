    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.4.1",
    "webpack-fix-style-only-entries": "^0.2.1"
  },
  "dependencies": {
    "evokit": "file:packages/evokit",
    "evokit-body": "file:packages/evokit-body",
    "evokit-box": "file:packages/evokit-box",
    "evokit-grid": "file:packages/evokit-grid",
    "evokit-image": "file:packages/evokit-image",
    "evokit-line": "file:packages/evokit-line",
