{
  "scripts": {
    "test": "lerna run test",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run --ignore evokit build && lerna run --scope evokit build",
    "watch": "lerna run --parallel --stream watch",
    "docs:build": "webpack --config webpack.docs.config.js && copyfiles ./packages/**/*.md docs",
    "docs:run": "webpack-dev-server --config webpack.docs.config.js",
    "docs:watch": "nodemon -e md,js,jsx --exec 'npm run docs:build'"
  },
  "nodemonConfig": {
    "ignore": [
      "docs/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/docccdev/evokit.git"
  },
  "keywords": [
    "uikit",
    "bem",
    "css",
    "stylus",
    "html",
    "jsx",
    "react"
  ],
  "author": "docccdev",
  "license": "MIT",
  "peerDependencies": {
    "prop-types": "*",
    "react": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-standalone": "^6.26.0",
    "copyfiles": "^2.1.0",
    "css-loader": "^1.0.0",
    "css-mqpacker": "^7.0.0",
    "docsify-demo-box-react": "^1.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.5.0",
    "lerna": "^3.14.1",
    "mini-css-extract-plugin": "^0.6.0",
    "nodemon": "^1.19.0",
    "postcss-conditionals": "^2.1.0",
    "postcss-custom-media": "^7.0.4",
    "postcss-custom-properties": "^8.0.8",
    "postcss-each": "^0.10.0",
    "postcss-for": "^2.1.1",
    "postcss-import": "^12.0.0",
